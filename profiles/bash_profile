source $(dirname $(dirname $(readlink ${HOME}/.bash_profile)))/loader.sh
# source ~/Workspace/git/environment/bash-loader.sh

[[ -s $(brew --prefix)/etc/profile.d/autojump.sh ]] && . $(brew --prefix)/etc/profile.d/autojump.sh

nvm use v8.12.0

. /Users/oeid/Workspace/binaries/miniconda3/etc/profile.d/conda.sh
conda activate cogenv3

export NVM_DIR="$HOME/Workspace/binaries/nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
